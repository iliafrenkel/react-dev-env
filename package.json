{
  "name": "react-dev-env",
  "version": "2.0.0",
  "description": "Example of development environment setup for React",
  "repository": {
    "type": "git",
    "url": "https://github.com/iliafrenkel/react-dev-env.git"
  },
  "main": "index.html",
  "scripts": {
    "postversion": "git push && git push --tags",
    "build": "./node_modules/.bin/webpack --env.env=prod",
    "build:prod": "./node_modules/.bin/webpack --env.env=prod",
    "build:dev": "./node_modules/.bin/webpack --env.env=dev",
    "watch": "./node_modules/.bin/webpack --watch --env.env=dev",
    "start": "node ./publish/server/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Ilia Frenkel <frenkel.ilia@gmail.com>",
  "license": "ISC",
  "private": false,
  "keywords": [
    "react",
    "environment",
    "setup",
    "webpack"
  ],
  "dependencies": {
    "body-parser": "^1.17.2",
    "express": "^4.15.3",
    "prop-types": "^15.5.10",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.23.0",
    "clean-webpack-plugin": "^0.1.15",
    "copy-webpack-plugin": "^4.2.3",
    "css-loader": "^0.28.4",
    "eslint": "^3.16.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "react-docgen": "^2.13.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^2.2.1",
    "webpack-merge": "^4.1.0"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  }
}
